# API keys
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
DEFAULT_KEY=

# Server
BACKEND_HOST=http://localhost
BACKEND_PORT=8000

# CORS
# Supports JSON array or comma-separated list, e.g. ["http://localhost:5173"]
CORS_ALLOW_ORIGINS=["*"]
CORS_ALLOW_METHODS=["*"]
CORS_ALLOW_HEADERS=["*"]
CORS_ALLOW_CREDENTIALS=false

# Output
OUTPUT_DIR=./instant-coffee-output

# Database
DATABASE_URL=sqlite:///./instant-coffee.db

# Feature flags
USE_LANGGRAPH=false
ENABLE_AESTHETIC_SCORING=false
ENABLE_MCP=false
LANGGRAPH_CHECKPOINTER=sqlite
LANGGRAPH_CHECKPOINT_URL=

# MCP
MCP_SERVER_URL=
MCP_SERVERS=
